{
  "RULE_MVD_": {
    ".*(регистрац|пропи[с|ш]|выпи[с|ш]).*": {
      "PARENT": "1.",
      "closein": "Прописка или выписка?",
      "deep": {
        ".*(пропис|всел|засел).*": {
          "closein": "Пребывание или жительство?",
          "deep": {
            ".*(житель|постоян).*": "1",
            ".*(пребыва|временн).*": "2"
          }
        },
        ".*(высел|выпис).*": {
          "closein": "Пребывание или жительство?",
          "deep": {
            ".*(житель|постоян).*": "3",
            ".*(пребыва|временн).*": "4"
          }
        }
      }
    },
    ".*(миграц|иностра|гост[и|ь]|погранич)|((?=.*(пересек|прибыл|прие[х|д]|прилет))(?=.*(границ|стран|украин|есвя[з|ж]))).*": {
      "PARENT": "2.",
      "closein": "Пребывание или жительство?",
      "deep": {
        ".*(житель|постоян).*": "2",
        ".*(пребыва|временн).*": "1"
      }
    },
    ".*(паспорт).*": {
      "PARENT": "3.",
      "closein": "Причина замены?",
      "deep": {
        "speech_num == 14": "1",
        ".*(неприг|негод|износ|поврежд|испор|испорчен|недейств).*": "2",
        "speech_num == 20 or speech_num == 45": "3",
        ".*((?=.*([з|с]мен))(?=.*(внешн))|существ).*": "4",
        ".*((?=.*([з|с]мен))(?=.*(пол))).*": "5",
        ".*(фио|имя|отчест|фамили|мест|рожде|дат[ы|а]|день|месяц|год|город|село|дерев|област).*": "6",
        ".*(неточн|ошиб).*": "7"
      }
    },
    ".*(загран|за загран|за предел).*": {
      "PARENT": "4./9.//",
      "closein": "Новый или старый?",
      "deep": {
        ".*(((?=.*(биометр|чип))(?=.*((\\bне)|(\\bбез))))|((?=.*((\\bна)|срок))(?=.*(пять)))|стар|деш[е|ё]в).*": {
          "PARENT": "4.",
          "closein": "Возраст",
          "deep": {
            "0<speech_num<14": "4.1",
            "14<=speech_num<18": "4.2",
            "18<=speech_num": "4.3"
          }
        },
        ".*(((?=.*((\\bна)|срок))(?=.*(десять)))|нов[о|ы]|доро[г|ж]|биометр|чип).*": {
          "PARENT": "9.",
          "closein": "Возраст",
          "deep": {
            "0<speech_num<14": "9.1",
            "14<=speech_num<18": "9.2",
            "18<=speech_num": "9.3"
          }
        }
      }
    },
    ".*(водитель| прав|автомобил|машин|транспорт).*": {
      "PARENT": "5.",
      "closein": "Причина замены?",
      "deep": {
        ".*(неприг|негод|износ|поврежд|испор|испорчен|недейств|измен|фио|имя|отчест|фамили|мест|рожде|дат[ы|а]|день|месяц|год|город|село|дерев|област|утрат|хищ).*": "1",
        ".*(срок|действ|здоров|состоян|медицин|огранич).*": "2",
        ".*(между|народ|интер).*": "3"
      }
    },
    ".*(нарушен|дорожн|движен).*": {
      "PARENT": "6.",
      "deep": "END"
    },
    ".*(судим|уголов).*": {
      "PARENT": "7.",
      "deep": "END"
    },
    ".*(нарко|психо[а|т]).*": {
      "PARENT": "8.",
      "deep": "END"
    }
  }
}